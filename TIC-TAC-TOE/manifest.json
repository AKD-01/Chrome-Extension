{
    "name": "Tic-tac-toe",
    "version": "1.0.0",
    "description": "A fun tic tac toe game for your browser.",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js":["copy.js"]
        }
    ],
    "browser_action": {
        "default_popup": "copy.html",
    },
    "icons": {
        "16": "icons/logo_16x16.png",
        "48": "icons/logo_48x48.png",
        "128": "icons/logo_128x128.png"
    },
    "manifest_version":2
}